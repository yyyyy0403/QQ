#include  <iostream>

using namespace std;

int main()

{
	int n;//宣告n 
	cin>>n;//輸入n 
if(n>=90)//如果n大於等於90 
{
	cout << "A" << endl;//輸出A	
}
else if(n>=80)//否則如果n大於等於80 
{
	cout << "B" << endl;//輸出B
}
else if(n>=70)//否則如果大於等於70 
{
	cout << "C" << endl;//輸出C 
}
else if(n>=60)//否則如果大於等於60 
{
	cout << "D" << endl;//輸出D 
}
else//上面都不符合就執行 
{
	cout << "E" << endl;//輸出E 
}
	
}
